{
    "commands": [
        {
            "Name": "moo",
            "Description": "cowpowers",
            "Type": "apt",
            "Examples": [
                "apt moo"
            ]
        },
        {
            "Name": "update",
            "Description": "updates the system repos",
            "Type": "apt",
            "Examples": [
                "sudo apt update"
            ]
        },
        {
            "Name": "ls",
            "Description": "list files and directories",
            "Type": "bash",
            "Examples": [
                "ls -a",
                "ls -alh",
                "ls dir1"
            ]
        },
        {
            "Name": "clear",
            "Description": "clears the terminal",
            "Type": "bash",
            "Examples": [
                "clear",
                "Ctrl+L"
            ]
        },
        {
            "Name": "nautilus",
            "Description": "launches gnome-files",
            "Type": "nautilus",
            "Examples": [
                "nautilus",
                "nautilus dir1",
                "nautilus -q"
            ]
        },
        {
            "Name": "find",
            "Description": "finds a filename using regex",
            "Type": "bash",
            "Examples": [
                "find . -type f -iname \"*papirus*\""
            ]
        },
        {
            "Name": "pgrep",
            "Description": "returns the pid of the program-name if it is opened else return nothing",
            "Type": "bash",
            "Examples": [
                "pgrep -u $(whoami) gnome-terminal",
                "pregp gedit"
            ]
        },
        {
            "Name": "pygmentize",
            "Description": "displays the contents of the file in a colored way according to the file's language",
            "Type": "pygmentize",
            "Examples": [
                "pip3 install --user Pygments",
                "pygmentize file.md",
                "pygmentize a.cpp",
                "cat a.cpp | pygmentize"
            ]
        },
        {
            "Name": "cat",
            "Description": "outputs the contents of a file to stdout or redirects it",
            "Type": "bash",
            "Examples": [
                "cat a.cpp",
                "cat file.txt > file2.txt",
                "cat file.txt >> file2.txt",
                "cat .bashrc"
            ]
        }
    ]
}